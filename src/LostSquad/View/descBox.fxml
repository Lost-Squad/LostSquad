<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="descBox" id="AnchorPane" minWidth="400" minHeight="200.0" prefHeight="200.0" prefWidth="440" style="-fx-background-color: #101010;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LostSquad.Controler.DescBoxController">
   <children>
      <Text fx:id="titleText" fill="#00ff37" layoutX="1.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description Box" textAlignment="CENTER" wrappingWidth="${(descBox.width)}">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Text>
      <Text fx:id="descText" fill="WHITE" layoutX="14.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="${(descBox.width-40)}">
         <font>
            <Font name="Bookman Old Style" size="18.0" />
         </font></Text>
   </children>
</AnchorPane>
